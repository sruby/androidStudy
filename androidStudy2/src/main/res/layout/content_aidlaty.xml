<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="io.github.sruby.aidl.AIDLAty"
    tools:showIn="@layout/activity_aidlaty"
    android:orientation="vertical">

    <Button
        android:layout_width="256dp"
        android:layout_height="wrap_content"
        android:text="启动androidStudy1中的service"
        android:id="@+id/btnStartAnotherAppService"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="223dp"
        android:layout_marginEnd="223dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="停止androidStudy1中的service"
        android:id="@+id/btnStopAnotherAppService"
        android:layout_below="@+id/btnStartAnotherAppService"
        android:layout_alignLeft="@+id/btnStartAnotherAppService"
        android:layout_alignStart="@+id/btnStartAnotherAppService" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="绑定androidStudy1中的service"
        android:id="@+id/btnBindAnotherAppService"
        android:layout_below="@+id/btnStartAnotherAppService"
        android:layout_alignLeft="@+id/btnStartAnotherAppService"
        android:layout_alignStart="@+id/btnStartAnotherAppService" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="解绑androidStudy1中的service"
        android:id="@+id/btnUnbindAnotherAppService"
        android:layout_below="@+id/btnStartAnotherAppService"
        android:layout_alignLeft="@+id/btnStartAnotherAppService"
        android:layout_alignStart="@+id/btnStartAnotherAppService" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="同步数据到androidStudy1中的service"
        android:id="@+id/btnSyncAnotherAppService"
        android:layout_alignLeft="@+id/btnStartAnotherAppService"
        android:layout_alignStart="@+id/btnStartAnotherAppService" />
</LinearLayout>
